{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "apuesta-chile/config",
    "type": "project",
    "description": "Unified responses format for Laravel projects",
    "keywords": [
        "laravel",
        "responses",
        "unified",
        "format"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "ApuestaChile\\Configurations\\": "src/Configurations/"
        },
        "files": [
            "src/Configurations/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ApuestaChile\\Configurations\\ServiceProvider"
            ]
        }
    }
}
